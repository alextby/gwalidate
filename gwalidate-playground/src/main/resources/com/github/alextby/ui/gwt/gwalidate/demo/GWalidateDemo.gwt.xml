<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="gwalidate_demo">

    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>

    <inherits name="com.github.gwtbootstrap.BootstrapNoResources"/>

    <source path="client"/>

    <entry-point class="com.github.alextby.ui.gwt.gwalidate.demo.client.DemoMain"/>

    <set-property name="gwt.logging.logLevel" value="SEVERE"/>
    <set-property name="gwt.logging.simpleRemoteHandler" value="ENABLED"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED"/>

    <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true"/>

    <extend-property name="locale" values="default"/>
    <extend-property name="locale" values="en"/>

    <define-configuration-property name="messageBundles" is-multi-valued="true"/>

</module>